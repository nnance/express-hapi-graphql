{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "outDir": "dist/",
        "typeRoots": [
            "node_modules/@types"
        ],
        "types": [
            "typed-graphql"
        ]
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}
